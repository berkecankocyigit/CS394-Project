<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/dark_gray" />
            <stroke
                android:width="4dp"
                android:color="@color/light_gray" />
